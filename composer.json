    "minimum-stability": "dev",
    "extra": {
        "oxideshop": {
            "assets-directory": "out/admin_twig",
            "blacklist-filter": [
                ".gitignore",
                "CHANGELOG-2.x.md",
                "composer.json"
            ],
            "target-directory": "admin_twig"
        }
    }
}
